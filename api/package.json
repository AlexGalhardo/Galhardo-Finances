{
	"name": "galhardo-finances-api",
	"version": "2.0.0",
	"description": "galhardo-app-api",
	"main": "dist/server.js",
	"author": "Alex Galhardo <aleexgvieira@gmail.com>",
	"license": "MIT",
	"engines": {
		"node": "v16.14.0",
		"npm": "8.19.2"
	},
	"prisma": {
		"seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
	},
	"scripts": {
		"start": "npm run build && node dist/server.js",
		"nodemon": "npx nodemon dist/server.js",
		"pm2": "npx pm2 start dist/server.js --name galhardo-app-api --interpreter node --watch",
		"dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
		"watch-compile": "swc src -w --out-dir dist",
		"watch-dev": "node ./dist/server.js",
		"build": "swc src -d dist",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:ci": "jest --passWithNoTests --runInBand --coverage",
		"vitest": "vitest",
		"vitest:coverage": "vitest run --coverage",
		"fix:prettier": "npx prettier --write src/*.ts src/**/*.ts src/**/**/*.ts src/**/**/**/*.ts tests/**/*.ts",
		"fix:lint": "npx eslint . --fix",
		"precommit": "rm -rf dist/ && npm run fix:prettier && npm run fix:lint",
		"delete:build": "rm -rf dist"
	},
	"dependencies": {
		"@prisma/client": "^4.8.0",
		"apollo-server": "^3.11.1",
		"bcryptjs": "^2.4.3",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-async-errors": "^3.1.1",
		"graphql": "^16.6.0",
		"helmet": "^6.0.1",
		"jsonwebtoken": "^9.0.0",
		"pm2": "^5.2.2",
		"prisma": "^4.8.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.205",
		"@types/bcryptjs": "^2.4.2",
		"@types/compression": "^1.7.2",
		"@types/express": "^4.17.13",
		"@types/fs-extra": "^9.0.13",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/node": "^18.11.17",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"@typescript-eslint/parser": "^5.19.0",
		"c8": "^7.11.3",
		"concurrently": "^7.2.2",
		"eslint": "^8.13.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-import-helpers": "^1.2.1",
		"eslint-plugin-prettier": "^4.0.0",
		"install": "^0.13.0",
		"jest": "^28.1.0",
		"npm": "^9.2.0",
		"prettier": "^2.6.2",
		"supertest": "^6.2.3",
		"ts-jest": "^28.0.2",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.9.4",
		"vitest": "^0.15.1"
	}
}
